}
.navbar{
    background-color: #202020;
    color: whitesmoke;
    height: 55px;
    width: 98.9vw;
}

.navbar>ul{
    float: right;
    width: 50vw;
    padding-right: 50px;
}

.navbar>ul>li{
    float: right;
    display: inline;
    list-style-type: none;
    margin: 0px 30px;
}
.navbar>ul>li>a:hover{
    animation: all 2s ease-in-out;
    border-bottom: 3px solid white;
    border-radius: 2px;
}
.navbar>ul>li>a{
    color: white;
    text-decoration: none;
    padding: 5px;
}
.profile{
