align-items: center;
}
.line{
    /* border: 1px solid red; */
   font-size: 3em;
    width: 12em;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    margin-top: -500px;
}
.Type-Writer{
    animation: typewriter 4s steps(40) 1s 1 normal both, 
    blinkTextcursor 400ms steps(40) infinite normal;
}
@keyframes typewriter{
from{
    width: 0;
}
to{
    width: 12em;
}
}
@keyframes blinkTextcursor{
from{
    border-right-color: rgba(0,0,0,0);
}
to{
    border-right-color: transparent;
    
}
}

